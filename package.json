    "build": "electron-builder"
  },
  "keywords": [],
  "author": "Elif Cakmak",
  "license": "Non-Commercial Redistribution License",
  "devDependencies": {
    "concurrently": "^9.1.1",
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8",
    "electron-reload": "^2.0.0-alpha.1"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "electron-updater": "^6.3.9"
  },
  "build": {
    "appId": "com.luma.app",
    "productName": "Luma",
    "files": [
      "app/**/*",
      "dist/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "output": "release"
    },
    "win": {
      "target": "nsis"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "effortless-quest",
        "repo": "luma"
      }
    ]
